{
  "name": "rehabify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:smoke": "vitest run stores env type-guards --reporter=dot --passWithNoTests",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "db:push": "dotenv -e .env.local -- drizzle-kit push",
    "db:studio": "dotenv -e .env.local -- drizzle-kit studio",
    "db:generate": "dotenv -e .env.local -- drizzle-kit generate",
    "db:setup": "./scripts/setup-database.sh",
    "db:migrate": "psql $DATABASE_URL -f src/db/migrations/001_complete_schema.sql",
    "db:seed": "psql $DATABASE_URL -f src/db/migrations/002_seed_data.sql",
    "db:mock": "psql $DATABASE_URL -f src/db/migrations/003_mock_data.sql",
    "db:generate-seed": "node scripts/generate-seed-sql.js"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "1eurofilter": "^1.2.2",
    "@dnd-kit/core": "^6.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.0.0",
    "@google/generative-ai": "^0.21.0",
    "@mediapipe/tasks-vision": "^0.10.14",
    "@neondatabase/auth": "0.1.0-beta.21",
    "@neondatabase/neon-js": "0.1.0-beta.22",
    "@neondatabase/serverless": "^0.10.0",
    "@radix-ui/react-slot": "^1.2.4",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.2",
    "@vapi-ai/web": "^2.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.38.0",
    "dynamic-time-warping": "^1.0.0",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.460.0",
    "next": "16.1.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "recharts": "^3.6.0",
    "tailwind-merge": "^2.5.0",
    "three": "^0.182.0",
    "zod": "^4.3.5",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^20",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@types/three": "^0.182.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "dotenv-cli": "^11.0.0",
    "drizzle-kit": "^0.30.0",
    "eslint": "^9",
    "eslint-config-next": "15.1.2",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "playwright": "^1.57.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vitest": "^4.0.15"
  }
}
